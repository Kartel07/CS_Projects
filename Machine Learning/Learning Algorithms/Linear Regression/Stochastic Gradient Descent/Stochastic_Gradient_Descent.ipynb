   "source": [
    "# Importing necessary libraries\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import pandas as pd"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "008363c8",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Loading and visualizing the dataset\n",
    "data = pd.read_csv('Salary_dataset.csv')\n",
    "plt.scatter(data.Experience,data.Salary)\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "3100423a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def NormalEquation():\n",
    "    x = np.array(data.Experience)\n",
    "    ones = np.ones(x.shape[0])\n",
    "    x = np.column_stack((x, ones))\n",
    "    y = np.array(data.Salary)\n",
    "    xT = x.transpose()\n",
    "    x_inv = np.linalg.inv(np.dot(xT, x))\n",
    "    theta = np.dot(np.dot(x_inv, xT), y)  # Normal Equation\n",
    "    return theta"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "d748904c",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Defining the hypothesis function\n",
    "def stochasticGradientDescent(theta0,theta1):\n",
    "    learning_rate = 0.0001\n",
    "    for i in range(len(data)):\n",
    "        x_i = data.Experience[i]\n",
    "        y_i = data.Salary[i]\n",
    "        hypothesis = theta0 + (theta1 * x_i)\n",
    "        theta0 = theta0 - learning_rate * (hypothesis - y_i)\n",
    "        theta1 = theta1 - learning_rate * (hypothesis - y_i) * x_i\n",
    "    return theta0,theta1"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "aec31a8c",
   "metadata": {},
   "outputs": [],
   "source": [
    "theta0 = 0\n",
    "theta1 = 0\n",
    "iterations = 45000\n",
    "for i in range(iterations):\n",
    "    theta0,theta1 = stochasticGradientDescent(theta0,theta1)\n",
    "    if(i % 500 == 0):\n",
    "        print(\"Theta values after\", i, \"iterations of Gradient Descent: theta0 =\", theta0, \", theta1 =\", theta1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "9580643a",
   "metadata": {},
   "outputs": [],
   "source": [
    "plt.scatter(data.Experience,data.Salary,color = 'white')\n",
    "plt.plot(list(range(0,12)), [theta0 + theta1*x for x in range(0,12)], color = 'red')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "c26c6792",
   "metadata": {},
   "outputs": [],
   "source": [
    "print(\"Comparing the results of Stochastic Gradient Descent with Normal Equation\") \n",
    "print(\"Gradient Stochastic Descent Thetas: \", np.array((np.array(theta1),np.array(theta0))))\n",
    "print(\"Normal Equation Thetas:\", NormalEquation())\n",
    "# Making predictions\n",
    "print(\"Predicted salary for 8 years of experience:\", theta0 + theta1*1.2)\n",
    "print(\"Absolute Error in thetas is:\")\n",
    "print(\"Theta0 Error:\", abs(NormalEquation()[0] - theta1))\n",
    "print(\"Theta1 Error:\", abs(NormalEquation()[1] - theta0))\n",
    "print(\"Relative Error in thetas is\")\n",
    "print(\"Theta0 Relative Error:\", abs(NormalEquation()[0] - theta1)/abs(NormalEquation()[0])*100,\"%\")\n",
    "print(\"Theta1 Relative Error:\", abs(NormalEquation()[1] - theta0)/abs(NormalEquation()[1])*100,\"%\")"
   ]
  }
 ],
